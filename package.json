{
  "name": "urlaubsverwaltung",
  "version": "1.0.0",
  "description": "Paperless absence management",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/main/webapp",
    "build": "NODE_ENV=production webpack --mode production",
    "build:dev": "NODE_ENV=development webpack --mode development",
    "build:watch": "NODE_ENV=development webpack --mode development --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synyx/urlaubsverwaltung.git"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/synyx/urlaubsverwaltung/issues"
  },
  "homepage": "https://github.com/synyx/urlaubsverwaltung#readme",
  "dependencies": {
    "@babel/polyfill": "7.10.1",
    "bootstrap": "3.4.1",
    "chosen-js": "1.8.7",
    "font-awesome": "4.7.0",
    "jquery": "3.5.1",
    "jquery-ui": "1.12.1",
    "jquery-ui-themes": "1.12.0",
    "list.js": "1.5.0",
    "tablesorter": "2.31.3",
    "timepicker": "1.11.14",
    "underscore": "1.10.2"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.10.2",
    "@urlaubsverwaltung/eslint-plugin": "1.1.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "css-loader": "3.5.3",
    "date-fns": "2.14.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-unicorn": "19.0.1",
    "fetch-mock": "9.10.1",
    "file-loader": "6.0.0",
    "imports-loader": "0.8.0",
    "jest": "26.0.1",
    "less": "3.11.2",
    "less-loader": "6.1.0",
    "merge": "1.2.1",
    "mini-css-extract-plugin": "0.9.0",
    "node-fetch": "2.6.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "terser-webpack-plugin": "3.0.2",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-assets-manifest": "3.1.1",
    "webpack-cli": "3.3.11"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "useBuiltIns": "entry",
          "corejs": "2",
          "targets": "> 0.25%, not dead"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/src/main/webapp"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.js",
      "!**/__tests__/**",
      "!**/WEB-INF/**"
    ],
    "coverageDirectory": "<rootDir>/target/js-coverage",
    "testURL": "http://localhost",
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
